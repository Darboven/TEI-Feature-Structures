<fsdDecl>
  <fsDecl type="set" baseTypes="mathobj text-phrase" xml:id="SET"><!-- mathobj needs to be defined -->
    <fsDescr>General set structure (defined by its subtypes)</fsDescr>
    <fDecl name="Variable">
      <vRange>
        <fs type="arbitrary-mathobj"/>
      </vRange>
    </fDecl>
    <fDecl name="Predicate">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="Text">
      <vRange>
        <vAlt>
          <fs name="set-comprehension"/>
          <fs name="set-enumeration"/>
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="set-comprehension" baseTypes="text-phrase" xml:id="SETCOMP">
    <fDecl name="Text">
      <vRange>
        <vLabel name="TR">
          <fs type="nonempty-text-phrase-list">
            <f name="Head">
              <fs type="text-phrase-leaf">
                <f name="Text">
                  <string>{</string>
                </f>
              </fs>
            </f>
            <f name="Tail">
              <fs type="nonempty-text-phrase-list">
                <f name="Head">
                  <fs type="variable-text-phrase">
                    <f name="Semantics">
                      <vLabel name="Variable">
                        <fs type="mathobj"/>
                      </vLabel>
                    </f>
                  </fs>
                </f>
                <f name="Tail">
                  <fs type="nonempty-text-phrase-list">
                    <f name="Head">
                      <fs type="text-phrase-leaf">
                        <f name="Text">
                          <vAlt>
                            <string>:</string>
                            <string>|</string>
                          </vAlt>
                        </f>
                      </fs>
                    </f>
                    <f name="Tail">
                      <fs type="nonempty-text-phrase-list">
                        <f name="Head">
                          <fs type="text-phrase-leaf">
                            <f name="Text">
                              <vLabel name="Predicate">
                                <fs type="proposition"/>
                              </vLabel>
                            </f>
                          </fs>
                        </f>
                        <f name="Tail">
                          <fs type="nonempty-text-phrase-list">
                            <f name="Head">
                              <fs type="text-phrase-leaf">
                                <f name="Text">
                                  <string>}</string>
                                </f>
                              </fs>
                            </f>
                            <f name="Tail">
                              <fs type="empty-text-phrase-list"/>
                            </f>
                          </fs>
                        </f>
                      </fs>
                    </f>
                  </fs>
                </f>
              </fs>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Semantics">
      <fs type="set">
        <f name="Variable">
          <vLabel name="Variable"/>
        </f>
        <f name="Predicate">
          <vLabel name="Predicate"/>
        </f>
        <f name="Text">
          <vLabel name="TR"/>
        </f>
      </fs>
    </fDecl>
  </fsDecl>


  <fsDecl type="set-enumeration" baseTypes="text-phrase" xml:id="SETENUM">
    <!-- !!! -->
    <fDecl name="Text">
      <vRange>
        <vLabel name="TR">
          <fs type="nonempty-text-phrase-list">
            <f name="Head">
              <fs type="comma-list-start"/>
            </f>
            <f name="Tail">
              <fs type="empty-text-phrase-list"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Semantics">
      <vRange>
        <fs type="set">
          <f name="Variable">
            <vLabel name="Variable"/>
          </f>
          <f name="Predicate">
            <fs type="disjunction-list">
              <f name="DIS">
                <fs type="var-equals-proposition-list">
                  <f name="Variable">
                    <vLabel name="Variable"/>
                  </f>
                  <f name="Mathobj-List">
                    <fs type="mathobj-list"/>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Text">
            <vLabel name="TR"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>

</fsdDecl>
