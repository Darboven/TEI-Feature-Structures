<fsdDecl>
  <fsDecl type="proof-contradiction" baseTypes="proved-under-hypotheses">
    <fDecl name="Initial-Linguistic-Marker">
      <vRange>
        <fs type="sentence">
          <f name="Linguistic-Indicator">
            <vAlt>
              <string>suppose for the sake of contradiction that it is not true that</string>
              <string>for the sake of contradiction, suppose</string>
              <string></string>
              <!-- ... -->
            </vAlt>
          </f>
          <!--
            Problem:
            bei manchen Formulierungen wird eine negierte Form der Proposition
            benÃ¶tigt, bei anderen jedoch nicht
          -->
          <f name="Linguistic-Realisation">
            <vLabel name="P">
              <fs type="proposition"/>
            </vLabel>
          </f>
        </fs>
      </vRange>
    </fDecl>
    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="T">
          <fs type="list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Conclusion">
      <vRange>
        <vLabel name="P"/>
      </vRange>
    </fDecl>
    <fDecl name="Proof">
      <vRange>
        <fs type="proved-under-hypotheses">
          <f name="Hypotheses">
            <fs type="list">
              <f name="Head">
                <fs type="negation">
                  <f name="Negation-Feature">
                    <vLabel name="P"/>
                  </f>
                </fs>
              </f>
              <f name="Tail">
                <vLabel name="T"/>
              </f>
            </fs>
          </f>
          <f name="Conclusion">
            <fs type="conjunction-lists">
              <f name="CON">
                <fs type="list">
                  <f name="Head">
                    <vLabel name="C"/>
                  </f>
                  <f name="Tail">
                    <fs type="list">
                      <f name="Head">
                        <fs type="negation">
                          <f name="Negation-Feature">
                            <vLabel name="C"/>
                          </f>
                        </fs>
                      </f>
                      <f name="Tail">
                        <fs type="empty-list"/>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>
</fsdDecl>
