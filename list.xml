<fsdDecl>

  <fsDecl type="list" xml:id="LIST">
    <fsDescr>
      General list structure
      (defined by its subtypes empty-list and nonempty-list)
    </fsDescr>
  </fsDecl>

  <fsDecl type="empty-list" baseTypes="list" xml:id="EMPL">
    <fsDescr>Empty list</fsDescr>
  </fsDecl>

  <fsDecl type="nonempty-list" baseTypes="list" xml:id="NEMPL">
    <fsDescr>Non-empty list</fsDescr>
    <fDecl name="Head"/>
    <fDecl name="Tail">
      <vRange>
        <fs type="list"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="proposition-list" baseTypes="list" xml:id="PLIST">
    <fsDescr>
      List of proposition
      (defined by its subtype nonempty-proposition-list)
    </fsDescr>
  </fsDecl>

  <fsDecl type="empty-proposition-list" baseTypes="proposition-list empty-list" xml:id="EMPPL"/>

  <fsDecl type="nonempty-proposition-list" baseTypes="proposition-list nonempty-list" xml:id="NEMPPL">
    <fsDescr>Non-empty list of propositions</fsDescr>
    <fDecl name="Head">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="list-with-spec-element" baseTypes="nonempty-list" xml:id="LWSE">
    <fsDescr>
      List that contains a specific element
      (defined by its subtypes list-with-spec-element-initially and
      list-with-spec-element-non-initially)
    </fsDescr>
    <fDecl name="Element"/>
  </fsDecl>

  <fsDecl type="list-with-spec-element-initially" baseTypes="list-with-spec-element" xml:id="LWSEI">
    <fsDescr>Part of the List with the specific Element</fsDescr>
    <fDecl name="Element">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
    <fDecl name="Head">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="list-with-spec-element-non-initially" baseTypes="list-with-spec-element" xml:id="LWSENI">
    <fDecl name="Element">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="list-with-spec-element">
          <f name="Element">
            <vLabel name="element"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="proposition-list-spec-element-initially" baseTypes="proposition-list list-with-spec-element-initially" xml:id="PLWSEI"/>

  <fsDecl type="proposition-list-spec-element-non-initially" baseTypes="proposition-list list-with-spec-element-non-initially" xml:id="PLWSENI"/>



  <fsDecl type="sublist" baseTypes="list" xml:id="SUBL">
    <fsDescr>List that is a sublist of a specific other list named Superlist (defined by its subtypes empty-sublist and nonempty-sublist)</fsDescr>
    <fDecl name="Superlist"/>
  </fsDecl>

  <fsDecl type="empty-sublist" baseTypes="sublist empty-list" xml:id="EMPSUBL">
  </fsDecl>

  <fsDecl type="nonempty-sublist" baseTypes="sublist nonempty-list" xml:id="NEMPSUBL">
    <fDecl name="SuperList">
      <vRange>
        <vLabel name="superlist">
          <fs type="list-with-spec-element">
            <f name="Element">
              <vLabel name="head"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Head">
      <vRange>
        <vLabel name="head"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="sublist">
          <f name="Superlist">
            <vLabel name="superlist"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="equivalent-list-constraint" xml:id="EQLC">
    <fsDescr>The constraint that List1 and List2 contain the same elements</fsDescr>
    <fDecl name="List1">
      <vRange>
        <vLabel name="list1">
          <fs type="sublist">
            <f name="Superlist">
              <vLabel name="list2"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="List2">
      <vRange>
        <vLabel name="list2">
          <fs type="sublist">
            <f name="Superlist">
              <vLabel name="list1"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
  </fsDecl>


</fsdDecl>
