<fsdDecl>
  <fsdLink type="proposition-list"
   target="list-tei-fsdecl#PLIST"/>
  <fsDecl type="proposition" xml:id="PROP">
    <fsDescr>General proposition structure (defined by its subtypes)</fsDescr>
  </fsDecl>

  <fsDecl type="atomic" baseTypes="proposition">
    <fDecl name="Content">
      <vRange>
        <symbol value="string"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="implication" baseTypes="proposition">
    <fDecl name="Antecedent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </f>
    <f name="Consequent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="biimplication" baseTypes="proposition">
    <fDecl name="BI-Feature-1">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </f>
    <fDecl name="BI-Feature-2">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="negation" baseTypes="proposition">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="conjunction-lists" baseTypes="proposition">
    <fDecl name="CON">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="disjunction-lists" baseTypes="proposition">
    <fDecl name="DIS">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="all-quantification" baseTypes="proposition">
    <fsDescr>All-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <symbol value="string"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="exists-quantification" baseTypes="proposition">
    <fsDescr>Exists-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <symbol value="string"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

</fsdDecl>
