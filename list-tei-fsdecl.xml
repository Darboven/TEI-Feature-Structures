<fsdDecl>

  <fsDecl type="list" xml:id="LIST">
    <fsDescr>General list structure (defined by its subtypes empty-list and nonempty-list)</fsDescr>
  </fsDecl>

  <fsDecl type="empty-list" baseTypes="list comma-list text-phrase-list">
    <fsDescr>Empty list</fsDescr>
  </fsDecl>

  <fsDecl type="nonempty-list" baseTypes="list">
    <fsDescr>Non-empty list</fsDescr>
    <fDecl name="Head"/>
    <fDecl name="Tail">
      <vRange>
        <fs type="list"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="proposition-list" baseTypes="list" xml:id="PLIST">
    <fsDescr>List of proposition (defined by its subtype nonempty-proposition-list)</fsDescr>
  </fsDecl>

  <fsDecl type="nonempty-proposition-list" baseTypes="proposition-list nonempty-list">
    <fsDescr>Non-empty list of propositions</fsDescr>
    <fDecl name="Head">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="list-with-spec-element" baseTypes="nonempty-list">
    <fsDescr>List that contains a specific element (defined by its subtypes list-with-spec-element-initially and list-with-spec-element-non-initially)</fsDescr>
    <fDecl name="Element"/>
  </fsDecl>

  <fsDecl type="list-with-spec-element-initially" baseTypes="list-with-spec-element">
    <fsDescr>Part of the List with the specific Element</fsDescr>
    <fDecl name="Element">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
    <fDecl name="Head">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="list-with-spec-element-non-initially" baseTypes="list-with-spec-element">
    <fDecl name="Element">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="list-with-spec-element">
          <f name="Element">
            <vLabel name="element"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>

<!--

-->

  <fsDecl type="comma-list" baseTypes="list">
    <fsDescr>General comma-list structure</fsDescr>
  </fsDecl>

  <fsDecl type="comma-list-start" baseTypes="comma-list">
    <fDecl name="Head">
      <vRange>
        <string>{</string>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <vAlt>
          <fs type="comma-list-element"/>
          <fs type="comma-list-end"/><!-- for empty set -->
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="comma-list-element" baseTypes="comma-list">
    <fDecl name="Head">
      <vRange>
        <fs type="mathobj"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <vAlt>
          <fs type="comma-list-comma"/>
          <fs type="comma-list-end"/>
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="comma-list-comma" baseTypes="comma-list">
    <fDecl name="Head">
      <vRange>
        <string>,</string>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="comma-list-element"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="comma-list-end" baseTypes="comma-list">
    <fDecl name="Head">
      <vRange>
        <string>}</string>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="empty-list"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="sublist" baseTypes="list">
    <fsDescr>List that is a sublist of a specific other list named Superlist (defined by its subtypes empty-sublist and nonempty-sublist)</fsDescr>
    <fDecl name="Superlist"/>
  </fsDecl>

  <fsDecl type="empty-sublist" baseTypes="sublist empty-list">
  </fsDecl>

  <fsDecl type="nonempty-sublist" baseTypes="sublist nonempty-list">
    <fDecl name="SuperList">
      <vRange>
        <vLabel name="superlist">
          <fs type="list-with-spec-element">
            <f name="Element">
              <vLabel name="head"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Head">
      <vRange>
        <vLabel name="head"/>
      </vRange>
    </fDecl>
    <fDecl name="Tail">
      <vRange>
        <fs type="sublist">
          <f name="Superlist">
            <vLabel name="superlist"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="equivalent-list-constraint">
    <fsDescr>The constraint that List1 and List2 contain the same elements</fsDescr>
    <fDecl name="List1">
      <vRange>
        <vLabel name="list1">
          <fs type="sublist">
            <f name="Superlist">
              <vLabel name="list2"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="List2">
      <vRange>
        <vLabel name="list2">
          <fs type="sublist">
            <f name="Superlist">
              <vLabel name="list1"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
  </fsDecl>

</fsdDecl>
