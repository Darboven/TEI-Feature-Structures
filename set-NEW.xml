<fsdDecl>
  <fsDecl type="set" baseTypes="mathobj"><!-- mathobj needs to be defined -->
    <fsDescr>General set structure (defined by its subtypes)</fsDescr>
    <fDecl name="Variable">
      <vRange>
        <vLabel name="variable">
          <fs type="atomic"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Predicate">
      <vRange>
        <vLabel name="predicate">
          <fs type="proposition"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Textual-Realisation">
      <vRange>
        <vAlt>
          <fs name="set-comprehension"/>
          <fs name="set-enumeration"/>
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="set-comprehension" baseTypes="text-phrase">
    <!-- !!! -->
    <fDecl name="Text">
      <vRange>
        <fs type="nonempty-text-phrase-list">
          <f name="Head">
            <fs type="text-phrase">
              <f name="Text">
                <string>{</string>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <fs type="nonempty-text-phrase-list">
              <f name="Head">
                <fs type="text-phrase">
                  Variable
                </fs>
              </f>
              <f name="Tail">
                <fs type="nonempty-text-phrase-list">
                  <f name="Head">
                    <fs type="text-phrase">
                      <f name="Text">
                        <vAlt>
                          <string>:</string>
                          <string>|</string>
                        </vAlt>
                      </f>
                    </fs>
                  </f>
                  <f name="Tail">
                    <fs type="nonempty-text-phrase-list">
                      <f name="Head">
                        <fs type="text-phrase">
                          Predicate
                        </fs>
                      </f>
                      <f name="Tail">
                        <fs type="nonempty-text-phrase-list">
                          <f name="Head">
                            <fs type="text-phrase">
                              <f name="Text">
                                <string>}</string>
                              </f>
                            </fs>
                          </f>
                          <f name="Tail">
                            <fs type="empty-text-phrase-list"/>
                          </f>
                        </fs>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>

