<fsdDecl>
  <fsDecl type="proof-using-cases" xml:id="PUC">

    <fDecl name="Text">
      <vRange>
        <fs type="nonempty-text-phrase-list">
          <f name="Head">
            <fs type="text-phrase-leaf">
              <f name="Text">
                <fs type="nonempty-text-phrase-list">
                  <f name="Head">
                    <fs type="text-phrase-leaf">
                      <f name="Text">
                        <vAlt>
                          <string>suppose </string>
                          <string>in conditional form, the proposition is as follows </string>
                        </vAlt>
                      </f>
                    </fs>
                  </f>
                  <f name="Tail">
                    <fs type="nonempty-text-phrase-list">
                      <f name="Head">
                        <vLabel name="P"/>
                      </f>
                      <f name="Tail">
                        <fs type="empty-text-phrase-list"/>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <fs type="nonempty-text-phrase-list">
              <f name="Head">
                <vLabel name="bew"/>
              </f>
              <f name="Tail">
                <fs type="empty-text-phrase-list"/>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="T"/>
      </vRange>
    </fDecl>

    <fDecl name="Conclusion">
      <vRange>
        <fs type="implication">
          <f name="Antecedent">
            <vLabel name="P">
              <fs type="disjunction-list">
                <f name="DIS">
                  <vLabel name="tbp">
                    <fs type="propostion-list"/>
                  </vLabel>
                </f>
              </fs>
            </vLabel>
          </f>
          <f name="Consequent">
            <vLabel name="Q"/>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Proof">
      <vRange>
        <fs type="proof-list">
          <f name="Hypotheses">
            <vLabel name="T"/>
          </f>
          <f name="Conclusion">
            <vLabel name="Q"/>
          </f>
          <f name="To-Prove-List">
            <vLabel name="tbp"/>
          </f>
        </fs>
      </vRange>
    </fDecl>

  </fsDecl>
</fsdDecl>
