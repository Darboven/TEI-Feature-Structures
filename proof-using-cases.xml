<fsdDecl>
  <fsDecl type="proof-using-cases" xml:id="PUC">

    <fDecl name="Text">
      <vRange>
        <fs type="nonempty-text-phrase-list">
          <f name="Head">
            <fs type="text-phrase-leaf">
              <f name="Text">
                <fs type="nonempty-text-phrase-list">
                  <f name="Head">
                    <fs type="text-phrase-leaf">
                      <f name="Text">
                        <vAlt>
                          <string>suppose </string>
                          <string>in conditional form, the proposition is as follows </string>
                        </vAlt>
                      </f>
                    </fs>
                  </f>
                  <f name="Tail">
                    <fs type="nonempty-text-phrase-list">
                      <f name="Head">
                        <vLabel name="P"/>
                      </f>
                      <f name="Tail">
                        <fs type="empty-text-phrase-list"/>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <fs type="nonempty-text-phrase-list">
              <f name="Head">
                <vLabel name="bew"/>
              </f>
              <f name="Tail">
                <fs type="empty-text-phrase-list"/>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Hypotheses">
      <vRange>
        <fs type="nonempty-proposition-list">
          <f name="Head">
            <fs type="disjunciton-list">
              <f name="DIS">
                <fs type="proposition-list-with-spec-element-non-initially">
                  <f name="Element">
                    <vLabel name="Pn"/>
                    <!-- last Element -->
                  </f>
                  <f name="Head">
                    <vLabel name="P1"/>
                  </f>
                  <f name="Tail">
                    <fs type="proposition-list-with-spec-element">
                      <f name="Element">
                        <vLabel name="Pn"/>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <vLabel name="T"/>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Conclusion">
      <vRange>
        <vLabel name="Q"/>
      </vRange>
    </fDecl>

    <fDecl name="Proof">
      <vRange>
        <fs type="proof-list-with-spec-element-non-initially">
          <f name="Element">
            <vLabel name="Pn"/><!-- last element to prove -->
          </f>
          <f name="Proved-Feature">
            <vLabel name="Q"/>
          </f>
          <f name="Head">
            <fs type="proved-under-hypotheses">
              <f name="Hypotheses">
                <vLabel name="T"/>
              </f>
              <f name="Conclusion">
                <fs type="implication">
                  <f name="Antecedent">
                    <vLabel name="P1"/>
                  </f>
                  <f name="Consequent">
                    <vLabel name="Q"/>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <fs type="proof-list-with-spec-element">
              <f name="Element">
                <vLabel name="Pn"/>
              </f>
              <f name="Proved-Feature">
                <vLabel name="Q"/>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>

  </fsDecl>
</fsdDecl>
