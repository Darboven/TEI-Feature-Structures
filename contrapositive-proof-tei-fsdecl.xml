<fsdDecl>
  <fsdLink type="proposition"
   target="proposition-tei-fsdecl.xml#PROP"/>
  <fsdLink type="list-with-spec-element"
   target="list-tei-fsdecl.xml#LWSE"/>
  <fsDecl type="contrapositive-proof" baseTypes="proved-under-hypotheses"
   xml:id="CONTRAPOSITIVE">
    <!--
      Idee:
        H: P|Ps
        C: Q
        P:  H: ~Q|Ps (aber als Liste?)
            C: ~P
    -->
    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="Ps"> <!-- zu viel, brauchen nur Tail? -->
          <fs type="list-with-spec-element">
            <f name="Element">
              <vLabel name="P">
                <fs type="proposition"/>
              </vLabel>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Conclusion">
      <vRange>
        <vLabel name="Q">
          <fs name="proposition"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Proof">
      <vRange>
        <fs type="proved-under-hypotheses">
          <f name="Hypotheses">
            <vLabel name="Ps">
              <fs type="list-with-spec-element">
                <f name="Element">
                  <fs type="negation">
                    <f name="Negation-Feature">
                      <vLabel name="Q"/>
                    </f>
                  </fs>
                </f>
              </fs>
            </vLabel>
          </f>
          <f name="Conclusion">
            <fs type="negation">
              <f name="Negation-Feature">
                <vLabel name="P"/>
              </f>
            </fs>
          </f>
          <f name="Proof">
            <symbol value="proved-under-hypotheses"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>
<fsdDecl>
