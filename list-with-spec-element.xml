<fsdDecl>
  <fsDecl type="list-with-spec-element" baseTypes="list" xml:id="LWSE">
    <fsDescr>(Checks if) a List contains a specified element.</fsDescr>
    <fDecl name="Element">
      <vRange>
        <vLabel name="element"/>
      </vRange>
    </fDecl>
    <fDecl type="List-Feature">
      <vRange>
        <vAlt>
          <vColl org="list">
            <vLabel name="element"/>
            <fs type="list"/>
          </vColl>
          <vColl org="list">
            <fs type="list-with-spec-element">
              <f name="Element">
                <vLabel name="element"/>
              </f>
            </fs>
          </vColl>
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>

  <!--
    überprüfen:
      - zyklisch durch vLabel?    ja
      - vLabel hier erlaubt?      nein
      - baseTypes="fs" sinnvoll?  nein
  -->
  <vLabel name="element">
    <fsDecl type="element-of" baseTypes="fs">
      <fDecl name="Containing-List">
        <fs type="list-with-spec-element">
          <f name="Element">
            <vLabel name="element"/>
          </f>
        </fs>
      </fDecl>
    </fsDecl>
  </vLabel>

  <fsDecl type="same-elts" xml:id="SAMEELTS">
    <fsDescr>(Checks if) two lists contain the same elements.</fsDescr>
    <fDecl name="List-1">
      <vRange>
        <vLabel name="L1">
          <fs type="list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="List-2">
      <vRange>
        <vLabel name="L2"/>
      </vRange>
    </fDecl>
    <fDecl name="Check-If-Same">
      <vRange>
        <!-- sind zwei feature structures erlaubt? -->
        <fs type="sublist">
          <f name="Super-List">
            <vLabel name="L1"/>
          </f>
          <f name="Containing-List">
            <vLabel name="L2"/>
          </f>
        </fs>
        <fs type="sublist">
          <f name="Super-List">
            <vLabel name="L2"/>
          </f>
          <f name="Containing-List">
            <vLabel name="L1"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="sublist" baseTypes="list">
    <fsDescr>(Checks if) one list is a sublist of another list.</fsDescr>
    <fDecl name="Super-List">
      <vRange>
        <vLabel name="SL">
          <fs type="list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Containing-List">
      <vRange>
        <vLabel name="CL">
          <fs type="list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl type="List-Feature">
      <vRange>
        <vAlt>
          <vColl org="list">
            <vLabel name="X">
              <fs type="element-of">
                <f name="Containing-List">
                  <vLabel name="CL"/>
                </f>
              </fs>
            </vLabel>
            <fs type="sublist">
              <f name="Super-List">
                <vLabel name="SL">
              </f>
            </fs>
          </vColl>
          <symbol value="emptylist"/>
        </vAlt>
      </vRange>
    </fDecl>
  <fsDecl>

</fsdDecl>
