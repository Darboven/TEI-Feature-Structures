<fsdDecl>

  <fsDecl type="proposition" xml:id="PROP">
    <fsDescr>General proposition structure (defined by its subtypes)</fsDescr>
  </fsDecl>


  <fsDecl type="implication" baseTypes="proposition" xml:id="PROPIMPL">
    <fDecl name="Antecedent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="Consequent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="implication-without-textual-realization" baseTypes="implication" xml:id="PROPIMPLWOTR"/>

  <fsDecl type="implication-with-textual-realization" baseTypes="implication text-phrase" xml:id="PROPIMPLTR"/>


  <fsDecl type="biimplication" baseTypes="proposition" xml:id="PROPBIIMPL">
    <fDecl name="BI-Feature-1">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="BI-Feature-2">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="biimplication-without-textual-realization" baseTypes="biimplication" xml:id="PROPBIIMPLWOTR"/>

  <fsDecl type="biimplication-with-textual-realization" baseTypes="biimplication text-phrase" xml:id="PROPBIIMPLTR"/>


  <fsDecl type="negation" baseTypes="proposition" xml:id="PROPNEG">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="negation-without-textual-realization" baseTypes="negation" xml:id="PROPNEGWOTR"/>

  <fsDecl type="negation-with-textual-realization" baseTypes="negation text-phrase" xml:id="PROPNEGTR"/>


  <fsDecl type="conjunction-list" baseTypes="proposition" xml:id="PROPCONL">
    <fDecl name="CON">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="conjunction-list-without-textual-realization" baseTypes="conjunction-list" xml:id="PROPCONLWOTR"/>

  <fsDecl type="conjunction-list-with-textual-realization" baseTypes="conjunction-list text-phrase" xml:id="PROPCONLTR"/>


  <fsDecl type="disjunction-list" baseTypes="proposition" xml:id="PROPDISL">
    <fDecl name="DIS">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="disjunction-list-without-textual-realization" baseTypes="disjunction-list" xml:id="PROPDISLWOTR"/>

  <fsDecl type="disjunction-list-with-textual-realization" baseTypes="disjunction-list text-phrase" xml:id="PROPDISLTR"/>


  <fsDecl type="all-quantification" baseTypes="proposition" xml:id="PROPALQ">
    <fsDescr>All-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <fs type="arbitrary-mathobj"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="all-quantification-without-textual-realization" baseTypes="all-quantification" xml:id="PROPALQWOTR"/>

  <fsDecl type="all-quantification-with-textual-realization" baseTypes="all-quantification text-phrase" xml:id="PROPALQTR"/>


  <fsDecl type="exists-quantification" baseTypes="proposition" xml:id="PROPEXQ">
    <fsDescr>Exists-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <fs type="arbitrary-mathobj"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="exists-quantification-without-textual-realization" baseTypes="exists-quantification" xml:id="PROPEXQWOTR"/>

  <fsDecl type="exists-quantification-with-textual-realization" baseTypes="exists-quantification text-phrase" xml:id="PROPEXQTR"/>


  <fsDecl type="equality" baseTypes="proposition" xml:id="PROPEQUAL">
    <fDecl name="LHS">
      <vRange>
        <fs type="mathobj"/>
      </vRange>
    </fDecl>
    <fDecl name="RHS">
      <vRange>
        <fs type="mathobj"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="equality-without-textual-realization" baseTypes="equality" xml:id="PROPEQUALWOTR"/>

  <fsDecl type="equality-with-textual-realization" baseTypes="equality text-phrase" xml:id="PROPEQUALTR"/>


  <fsDecl type="subset" baseTypes="proposition" xml:id="PROPSUB">
    <fDecl name="Subset">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
    <fDecl name="Superset">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="subset-without-textual-realization" baseTypes="subset" xml:id="PROPSUBWOTR"/>

  <fsDecl type="subset-with-textual-realization" baseTypes="subset text-phrase" xml:id="PROPSUBTR"/>


  <fsDecl type="element" baseTypes="proposition" xml:id="PROPELEM">
    <fDecl name="Element">
      <vRange>
        <fs type="mathobj"/>
        <!-- most general ontological frame -->
      </vRange>
    </fDecl>
    <fDecl name="Set">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="element-without-textual-realization" baseTypes="element" xml:id="PROPELEMWOTR"/>

  <fsDecl type="element-with-textual-realization" baseTypes="element text-phrase" xml:id="PROPELEMTR"/>


  <fsDecl type="rational" baseTypes="proposition" xml:id="PROPRAT">
    <fDecl name="Argument">
      <vRange>
        <fs type="real-number"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="rational-without-textual-realization" baseTypes="rational" xml:id="PROPRATWOTR"/>

  <fsDecl type="rational-with-textual-realization" baseTypes="rational text-phrase" xml:id="PROPRATTR"/>


  <fsDecl type="irrational" baseTypes="proposition negation" xml:id="PROPIRR">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="rational"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="irrational-without-textual-realization" baseTypes="irrational" xml:id="PROPIRRWOTR"/>

  <fsDecl type="irrational-with-textual-realization" baseTypes="irrational text-phrase" xml:id="PROPIRRTR"/>


  <fsDecl type="even" baseTypes="proposition" xml:id="PROPEVEN">
    <fDecl name="Argument">
      <vRange>
        <fs type="integer"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="even-without-textual-realization" baseTypes="even" xml:id="PROPEVENWOTR"/>

  <fsDecl type="even-with-textual-realization" baseTypes="even text-phrase" xml:id="PROPEVENTR"/>


  <fsDecl type="odd" baseTypes="proposition negation" xml:id="PROPODD">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="even"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="odd-without-textual-realization" baseTypes="odd" xml:id="PROPODDWOTR"/>

  <fsDecl type="odd-with-textual-realization" baseTypes="odd text-phrase" xml:id="PROPODDTR"/>


  <fsDecl type="greater-equal" baseTypes="proposition" xml:id="PROPGREQ">
    <fDecl name="Greater">
      <vRange>
        <vAlt>
          <fs type="natural-number"/>
          <fs type="integer"/>
          <fs type="real-number"/>
        </vAlt>
      </vRange>
    </fDecl>
    <fDecl name="Lesser">
      <vRange>
        <vAlt>
          <fs type="natural-number"/>
          <fs type="integer"/>
          <fs type="real-number"/>
        </vAlt>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="greater-equal-without-textual-realization" baseTypes="greater-equal" xml:id="PROPGREQWOTR"/>

  <fsDecl type="greater-equal-with-textual-realization" baseTypes="greater-equal text-phrase" xml:id="PROPGREQTR"/>


</fsdDecl>
