<fsdDecl>

  <fsDecl type="proposition" baseTypes="text-phrase" xml:id="PROP">
    <fsDescr>General proposition structure (defined by its subtypes)</fsDescr>
  </fsDecl>

  <fsDecl type="implication" baseTypes="proposition" xml:id="PROPIMPL">
    <fDecl name="Antecedent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="Consequent">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="biimplication" baseTypes="proposition" xml:id="PROPBIIMPL">
    <fDecl name="BI-Feature-1">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
    <fDecl name="BI-Feature-2">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="negation" baseTypes="proposition" xml:id="PROPNEG">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="conjunction-list" baseTypes="proposition" xml:id="PROPCONL">
    <fDecl name="CON">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="disjunction-list" baseTypes="proposition" xml:id="PROPDISL">
    <fDecl name="DIS">
      <vRange>
        <fs type="proposition-list"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="all-quantification" baseTypes="proposition" xml:id="PROPALQ">
    <fsDescr>All-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <fs type="arbitrary-mathobj"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="exists-quantification" baseTypes="proposition" xml:id="PROPEXQ">
    <fsDescr>Exists-Quantification.</fsDescr>
    <fDecl name="Quantified-Variable">
      <vRange>
        <fs type="arbitrary-mathobj"/>
        <!-- needs work -->
      </vRange>
    </fDecl>
    <fDecl name="Quantified-Proposition">
      <vRange>
        <fs type="proposition"/>
      </vRange>
    </fDecl>
  </fsDecl>


  <fsDecl type="atomic" baseTypes="proposition" xml:id="PROPATOM">
    <fsDescr>General atomic structure</fsDescr>
  </fsDecl>

  <fsDecl type="equality" baseTypes="atomic" xml:id="PROPEQUAL">
    <fDecl name="LHS">
      <vRange>
        <fs type="mathobj"/>
      </vRange>
    </fDecl>
    <fDecl name="RHS">
      <vRange>
        <fs type="mathobj"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <!-- ... -->

  <fsDecl type="subset" baseTypes="atomic" xml:id="PROPSUB">
    <fDecl name="Subset">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
    <fDecl name="Superset">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="element" baseTypes="atomic" xml:id="PROPELEM">
    <fDecl name="Element">
      <vRange>
        <fs type="mathobj"/>
        <!-- most general ontological frame -->
      </vRange>
    </fDecl>
    <fDecl name="Set">
      <vRange>
        <fs type="set"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="rational" baseTypes="atomic" xml:id="PROPRAT">
    <fDecl name="Argument">
      <vRange>
        <fs type="real-number"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="irrational" baseTypes="atomic negation" xml:id="PROPIRR">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="rational"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="even" baseTypes="atomic" xml:id="PROPEVEN">
    <fDecl name="Argument">
      <vRange>
        <fs type="integer"/>
      </vRange>
    </fDecl>
  </fsDecl>

  <fsDecl type="odd" baseTypes="atomic negation" xml:id="PROPODD">
    <fDecl name="Negation-Feature">
      <vRange>
        <fs type="even"/>
      </vRange>
    </fDecl>
  </fsDecl>

</fsdDecl>
