<fsdDecl>
  <fsDecl type="proof-direct" baseTypes="proved-under-hypotheses">
    <fDecl name="Initial-Linguistic-Marker">
     <vRange>
       <fs type="sentence">
         <f name="Linguistic-Indicator">
           <vAlt>
             <string>suppose</string>
             <string>assume</string>
             <string>assume that</string>
           </vAlt>
         </f>
         <f name="Linguistic-Realisation">
           <vLabel name="P">
             <fs type="proposition"/>
           </vLabel>
         </f>
       </fs>
     </vRange>
    </fDecl>
    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="T">
          <fs type="proposition-list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Conclusion">
      <vRange>
        <fs type="implication">
          <f name="Antecedent">
            <vLabel name="P"/>
          </f>
          <f name="Consequent">
            <vLabel name="Q">
              <fs type="proposition"/>
            </vLabel>
          </f>
        </fs>
      </vRange>
    </fDecl>
    <fDecl name="Proof">
      <vRange>
        <fs type="proved-under-hypotheses">
          <f name="Hypotheses">
            <fs type="list">
              <f name="Head">
                <vLabel name="P"/>
              </f>
              <f name="Tail">
                <vLabel name="T"/>
              </f>
            </fs>
          </f>
          <f name="Conclusion">
            <vLabel name="Q"/>
          </f>
        </fs>
      </vRange>
    </fDecl>
  </fsDecl>
</fsdDecl>
