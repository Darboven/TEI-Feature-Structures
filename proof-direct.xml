<fsdDecl>
  <fsDecl type="proof-direct" baseTypes="proved-under-hypotheses" xml:id="PDIR">

    <fDecl name="Text">
      <vRange>
        <fs type="nonempty-text-phrase-list">
          <f name="Head">
            <fs type="text-phrase-leaf">
              <f name="Text">
                <fs type="nonempty-text-phrase-list">
                  <f name="Head">
                    <fs type="text-phrase-leaf">
                      <f name="Text">
                        <vAlt>
                          <string/>
                          <string>suppose </string>
                          <string>assume </string>
                          <string>assume that </string>
                        </vAlt>
                      </f>
                    </fs>
                  </f>
                  <f name="Tail">
                    <fs type="nonempty-text-phrase-list">
                      <f name="Head">
                        <vLabel name="P">
                          <fs type="proposition"/>
                        </vLabel>
                      </f>
                      <f name="Tail">
                        <fs type="empty-text-phrase-list"/>
                      </f>
                    </fs>
                  </f>
                </fs>
              </f>
            </fs>
          </f>
          <f name="Tail">
            <fs type="nonempty-text-phrase-list">
              <f name="Head">
                <vLabel name="bew"/>
              </f>
              <f name="Tail">
                <fs type="empty-text-phrase-list"/>
              </f>
            </fs>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="T">
          <fs type="proposition-list"/>
        </vLabel>
      </vRange>
    </fDecl>

    <fDecl name="Conclusion">
      <vRange>
        <fs type="implication">
          <f name="Antecedent">
            <vLabel name="P"/>
          </f>
          <f name="Consequent">
            <vLabel name="Q">
              <fs type="proposition"/>
            </vLabel>
          </f>
        </fs>
      </vRange>
    </fDecl>

    <fDecl name="Proof">
      <vRange>
        <vLabel name="bew">
          <fs type="proved-under-hypotheses">
            <f name="Hypotheses">
              <fs type="list">
                <f name="Head">
                  <vLabel name="P"/>
                </f>
                <f name="Tail">
                  <vLabel name="T"/>
                </f>
              </fs>
            </f>
            <f name="Conclusion">
              <vLabel name="Q"/>
            </f>
          </fs>
        </vLabel>
      </vRange>
    </fDecl>

  </fsDecl>
</fsdDecl>
