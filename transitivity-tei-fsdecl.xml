<fsdDecl>
  <fsDecl type="transitivity" baseTypes="proved-under-hypotheses" xml:id="PTRANS">
    <fsDescr>Allows to transitively concatenate and proof propositions.</fsDescr>
    <fDecl name="Hypotheses">
      <vRange>
        <vLabel name="Ps">
          <fs type="proposition-list"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Conclusion">
      <vRange>
        <vLabel name="R">
          <fs name="proposition"/>
        </vLabel>
      </vRange>
    </fDecl>
    <fDecl name="Proof">
      <vRange>
        <vColl>
          <fs type="proved-under-hypotheses">
            <f name="Hypotheses">
              <vLabel name="Ps"/>
            </f>
            <f name="Conclusion">
              <vLabel name="Q"/>
            </f>
          </fs>
          <fs type="proved-under-hypotheses">
            <f name="Hypotheses">
              <vColl org="list">
                <vLabel name="Q"/>
                <vLabel name="Ps"/>
              </vColl>
            </f>
            <f name="Conclusion">
              <vLabel name="R"/>
            </f>
          </fs>
        </vColl>
      </vRange>
    </fDecl>
  </fsDecl>
</fsdDecl>
